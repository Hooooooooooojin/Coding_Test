    "def solution(files):\n",
    "\n",
    "    import re\n",
    "\n",
    "    answer = [re.split(r'([0-9]+)',i) for i in files] # 정규 표현식을 활용해 숫자인 걸 기준으로 split해 list 생성\n",
    "\n",
    "    answer.sort(key=lambda x:(x[0].lower(),int(x[1]))) # head, number 기준 정렬\n",
    "\n",
    "    answer = [''.join(i) for i in answer] # 다시 Join\n",
    "\n",
    "    return answer"
   ]
  }
